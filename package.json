{
  "name": "finddx",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^2.38.0",
    "@azure/msal-react": "^1.5.9",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@gnomad/region-viewer": "^3.0.0",
    "@gnomad/track-variants": "^1.0.2",
    "@hookform/resolvers": "^2.8.10",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.7.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@sentry/react": "^7.120.1",
    "@sentry/tracing": "^7.19.0",
    "@tanstack/react-table": "^8.5.1",
    "@tanstack/react-virtual": "^3.0.0-beta.18",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-test-renderer": "^18.0.0",
    "async-mutex": "^0.3.2",
    "axios": "^1.7.4",
    "canvas": "^2.9.3",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-geo": "^3.1.0",
    "d3-geo-scale-bar": "^1.2.5",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.29.3",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18.1.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.34.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.0.1",
    "react-virtual": "^2.10.4",
    "recharts": "^2.1.10",
    "styled-components": "^5.3.5",
    "ts-pattern": "^4.0.3",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-local-domain": "sudo env PORT=443 HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test-watch": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "test-update-snapshots": "react-scripts test -u",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint .",
    "g": "plop"
  },
  "eslintConfig": {
    "rules": {
      "react/require-default-props": "off",
      "react/jsx-props-no-spreading": "off",
      "import/prefer-default-export": "off",
      "react-hooks/exhaustive-deps": "warn",
      "no-param-reassign": "off",
      "no-bitwise": "off",
      "import/no-named-as-default": "off",
      "linebreak-style": "off",
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ]
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "extends": [
      "react-app",
      "eslint-config-airbnb",
      "eslint-config-airbnb/hooks",
      "airbnb-typescript",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/d3": "^7.4.0",
    "@types/d3-geo": "^3.0.3",
    "@types/react-csv": "^1.1.3",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "plop": "^3.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^d3-(.*)$": "d3-$1/dist/d3-$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios|react-toastify)/)"
    ]
  }
}
